@model lab2.Presentation.Account.AccountProductsViewModel
<div class="w-full">
  <main class="profile-page">
    <section class="relative block" style="height: 500px;">
      <div
          class="absolute top-0 w-full h-full bg-center bg-cover"
          style='background-image: url("https://images.unsplash.com/photo-1499336315816-097655dcfbda?ixlib=rb-1.2.1&amp;ixid=eyJhcHBfaWQiOjEyMDd9&amp;auto=format&amp;fit=crop&amp;w=2710&amp;q=80");'
      >
          <span
              id="blackOverlay"
              class="w-full h-full absolute opacity-50 bg-black"
          ></span>
      </div>
      <div
          class="top-auto bottom-0 left-0 right-0 w-full absolute pointer-events-none overflow-hidden"
          style="height: 70px; transform: translateZ(0px);"
      >
        <svg
            class="absolute bottom-0 overflow-hidden"
            xmlns="http://www.w3.org/2000/svg"
            preserveAspectRatio="none"
            version="1.1"
            viewBox="0 0 2560 100"
            x="0"
            y="0"
        >
          <polygon
              class="text-gray-300 fill-current"
              points="2560 0 2560 100 0 100"
          ></polygon>
        </svg>
      </div>
    </section>
    <section class="relative py-16 bg-gray-300">
      <div class="container mx-auto px-4">
        <div
            class="relative flex flex-col min-w-0 break-words bg-white w-full mb-6 shadow-xl rounded-lg -mt-64"
        >
          <div class="px-6">
            <div class="flex flex-wrap justify-center">
              <div
                  class="w-full lg:w-3/12 px-4 lg:order-2 flex justify-center"
              >
                <div class="relative">
                  <img
                      alt="..."
                      :src="'/Image/'+team.Img"
                      class="shadow-xl rounded-full h-auto align-middle border-none absolute -m-16 -ml-20 lg:-ml-16"
                      style="max-width: 150px;"
                  />
                </div>
              </div>
              <div
                  class="w-full lg:w-4/12 px-4 lg:order-3 lg:text-right lg:self-center"
              >
                <div class="py-6 px-3 mt-32 sm:mt-0">
                  <slot name="button"></slot>
                </div>
              </div>
              <div class="w-full lg:w-4/12 px-4 lg:order-1">
                <div class="flex justify-center py-4 lg:pt-4 pt-8">
                  <div class="mr-4 p-3 text-center" v-if="ftitle">
                      <span
                          class="text-xl font-bold block uppercase tracking-wide text-gray-700"
                      >$@Model.total</span
                      ><span class="text-sm text-gray-500">Total Ammount Money</span>
                  </div>
                  <div class="mr-4 p-3 text-center" v-if="stitle">
                      <span 
                          class="text-xl font-bold block uppercase tracking-wide text-gray-700"
                      >@Model.totalAccounts</span
                      ><span class="text-sm text-gray-500">N Accounts</span>
                  </div>
                  <div class="lg:mr-4 p-3 text-center" v-if="ttitle">
                      <span
                          class="text-xl font-bold block uppercase tracking-wide text-gray-700"
                      >@Model.user.Id</span
                      ><span class="text-sm text-gray-500">User's Id</span>
                  </div>
                </div>
              </div>
            </div>
            <div class="text-center mt-12">
                <h3
                    class="text-4xl font-semibold leading-normal mb-2 text-gray-800 mb-2">
                    @Model.user.Name
                </h3>
                
              <div
                  class="text-sm leading-normal mt-0 mb-2 text-gray-500 font-bold uppercase"
              >
                <i
                    class="fas fa-map-marker-alt mr-2 text-lg text-gray-500"
                ></i>
                    @Model.cuentaBancaria.NCuenta
              </div>
              <!--<div class="mb-2 text-gray-700 mt-10">
                <i class="fas fa-briefcase mr-2 text-lg text-gray-500"></i
                >Solution Manager - Creative Tim Officer
              </div>-->
             <!--<div class="mb-2 text-gray-700">
                <i class="fas fa-university mr-2 text-lg text-gray-500"></i
                >University of Computer Science
              </div>-->
            </div>
            <div class="mt-10 py-10 border-t border-gray-300 text-center">
              <div class="flex flex-wrap justify-center">
                <div class="w-full lg:w-9/12 px-4">
                  <!--#####################################################################################################-->
                  <div class="flex flex-col">
                    
                    <a asp-controller="CheckingAccount" asp-action="Index" asp-route-IdBankAccount="@Model.cuentaBancaria.Id" type="button" class="ml-5 focus:outline-none text-white text-sm py-2.5 px-5 h-1/2 border-b-4 border-green-600 rounded-md bg-green-500 hover:bg-green-400">Manage</a>
                    <div
                      class="fas fa-map-marker-alt mr-2 text-lg text-gray-500">
                      Checking Accounts
                    </div>

                    <div class="-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
                      <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
                        <div class="shadow overflow-hidden border-b border-gray-200 sm:rounded-lg">
                          <table class="min-w-full divide-y divide-gray-200">
                            <thead class="bg-gray-50">
                            <tr>
                              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Id
                              </th>
                              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Id Bank Account
                              </th>
                              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Ammount 
                              </th>
                              <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Status
                              </th>
                              <th scope="col" class="relative px-6 py-3">
                                <span class="sr-only">Edit</span>
                              </th>
                            </tr>
                            </thead>
                            <tbody class="bg-white divide-y divide-gray-200">
                            @if (@Model.CuentaCorrientes != null)
                            {
                              @foreach (var checkingAccount in @Model.CuentaCorrientes)
                              {
                                <tr>
                                  <td class="px-6 py-4 whitespace-nowrap">
                                    <div class="flex items-center">
                                      <div class="flex-shrink-0 h-10 w-10">
                                      </div>
                                      <div class="ml-4">
                                        <div class="text-sm font-medium text-gray-900">
                                          @checkingAccount.Id
                                        </div>
                                        <div class="text-sm text-gray-500">
                                          @Model.user.Name
                                        </div>
                                      </div>
                                    </div>
                                  </td>
                                  <td class="px-6 py-4 whitespace-nowrap">
                                    <div class="text-sm text-gray-900">@checkingAccount.IdCuentaBancaria</div>
                                  </td>
                                  <td class="px-6 py-4 whitespace-nowrap">
                                  <div class="text-sm text-gray-900">$@checkingAccount.Saldo</div>
                                </td>
                                  <td class="px-6 py-4 whitespace-nowrap">
                                    <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800">
                                      Active
                                    </span>
                                  </td>
                                  <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
                                    <a href="#" class="text-indigo-600 hover:text-indigo-900">Edit</a>
                                  </td>
                                </tr>
                              }
                            }

                            <!-- More people... -->
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- ################################################################################################-->
                  <!-- CUENTAS DE AHORRO -->
                  <!--#####################################################################################################-->
                                    <div class="flex flex-col">
                                      
                                      <a asp-controller="SavingAccount" asp-action="Index" asp-route-IdBankAccount="@Model.cuentaBancaria.Id" type="button" class="ml-5 focus:outline-none text-white text-sm py-2.5 px-5 h-1/2 border-b-4 border-green-600 rounded-md bg-green-500 hover:bg-green-400">Manage</a>
                                      <div
                                        class="fas fa-map-marker-alt mr-2 text-lg text-gray-500">
                                        Saving Accounts 
                                      </div>
                  
                                      <div class="-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
                                        <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
                                          <div class="shadow overflow-hidden border-b border-gray-200 sm:rounded-lg">
                                            <table class="min-w-full divide-y divide-gray-200">
                                              <thead class="bg-gray-50">
                                              <tr>
                                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                  Id
                                                </th>
                                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                  Id Bank Account
                                                </th>
                                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                                Ammount 
                                                                              </th>
                                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                  Status
                                                </th>
                                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                 Interest Rate 
                                                </th>
                                                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                 Profits 
                                                </th>
                                                <th scope="col" class="relative px-6 py-3">
                                                  <span class="sr-only">Edit</span>
                                                </th>
                                              </tr>
                                              </thead>
                                              <tbody class="bg-white divide-y divide-gray-200">
                                              @if (@Model.CuentasAhorro != null)
                                              {
                                                @foreach (var savingAccount in @Model.CuentasAhorro)
                                                {
                                                  <tr>
                                                    <td class="px-6 py-4 whitespace-nowrap">
                                                      <div class="flex items-center">
                                                        <div class="flex-shrink-0 h-10 w-10">
                                                        </div>
                                                        <div class="ml-4">
                                                          <div class="text-sm font-medium text-gray-900">
                                                            @savingAccount.Id
                                                          </div>
                                                          <div class="text-sm text-gray-500">
                                                            @Model.user.Name
                                                          </div>
                                                        </div>
                                                      </div>
                                                    </td>
                                                    <td class="px-6 py-4 whitespace-nowrap">
                                                      <div class="text-sm text-gray-900">@savingAccount.IdCuentaBancaria</div>
                                                    </td>
                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                       <div class="text-sm text-gray-900">$@savingAccount.Saldo</div>
                                                     </td>                                 
                                                    <td class="px-6 py-4 whitespace-nowrap">
                                                      <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800">
                                                        Active
                                                      </span>
                                                    </td>
                                                    <td class="px-6 py-4 whitespace-nowrap">
                                                      <div class="text-sm text-gray-900">@savingAccount.TasaInteresMensual</div>
                                                    </td>
                                                    <td class="px-6 py-4 whitespace-nowrap">
                                                   <div class="text-sm text-gray-900">$@savingAccount.Ganacias</div>
                                                 </td>
                                                    <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
                                                      <a href="#" class="text-indigo-600 hover:text-indigo-900">Edit</a>
                                                    </td>
                                                  </tr>
                                                }
                                              }
                  
                                              <!-- More people... -->
                                              </tbody>
                                            </table>
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                <!-- ################################################################################################-->
                <!-- DEPOSITO A PLAZO -->
                  <!--#####################################################################################################-->
                                                      <div class="flex flex-col">
                                                        
                                                        <a asp-controller="TimeDeposit" asp-action="Index" asp-route-IdBankAccount="@Model.cuentaBancaria.Id" type="button" class="ml-5 focus:outline-none text-white text-sm py-2.5 px-5 h-1/2 border-b-4 border-green-600 rounded-md bg-green-500 hover:bg-green-400">Manage</a>
                                                        <div
                                                          class="fas fa-map-marker-alt mr-2 text-lg text-gray-500">
                                                          Time Deposits 
                                                        </div>
                                    
                                                        <div class="-my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
                                                          <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
                                                            <div class="shadow overflow-hidden border-b border-gray-200 sm:rounded-lg">
                                                              <table class="min-w-full divide-y divide-gray-200">
                                                                <thead class="bg-gray-50">
                                                                <tr>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                    Id
                                                                  </th>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                    Id Bank Account
                                                                  </th>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                                                  Ammount 
                                                                                                </th>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                    Status
                                                                  </th>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                    Interest Rate
                                                                  </th>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                    Days 
                                                                  </th>
                                                                  <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                                    Profits 
                                                                  </th>
                                                                  <th scope="col" class="relative px-6 py-3">
                                                                    <span class="sr-only">Edit</span>
                                                                  </th>
                                                                </tr>
                                                                </thead>
                                                                <tbody class="bg-white divide-y divide-gray-200">
                                                                @if (@Model.DepositoAPlazos != null)
                                                                {
                                                                  @foreach (var depositoAPlazo in @Model.DepositoAPlazos)
                                                                  {
                                                                    <tr>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                        <div class="flex items-center">
                                                                          <div class="flex-shrink-0 h-10 w-10">
                                                                          </div>
                                                                          <div class="ml-4">
                                                                            <div class="text-sm font-medium text-gray-900">
                                                                              @depositoAPlazo.Id
                                                                            </div>
                                                                            <div class="text-sm text-gray-500">
                                                                              @Model.user.Name
                                                                            </div>
                                                                          </div>
                                                                        </div>
                                                                      </td>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                        <div class="text-sm text-gray-900">@depositoAPlazo.IdCuentaBancaria</div>
                                                                      </td>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                      <div class="text-sm text-gray-900">$@depositoAPlazo.Cantidad</div>
                                                                      </td>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                        <span class="px-2 inline-flex text-xs leading-5 font-semibold rounded-full bg-green-100 text-green-800">
                                                                          Active
                                                                        </span>
                                                                      </td>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                    <div class="text-sm text-gray-900">@depositoAPlazo.TasaInteres</div>
                                                                    </td>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                        <div class="text-sm text-gray-900">@depositoAPlazo.CantidadDias</div>
                                                                      </td>
                                                                      <td class="px-6 py-4 whitespace-nowrap">
                                                                        <div class="text-sm text-gray-900">$@depositoAPlazo.Ganacias</div>
                                                                      </td>
                                                                                                                                            
                                                                      <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
                                                                        <a href="#" class="text-indigo-600 hover:text-indigo-900">Edit</a>
                                                                      </td>
                                                                    </tr>
                                                                  }
                                                                }
                                    
                                                                <!-- More people... -->
                                                                </tbody>
                                                              </table>
                                                            </div>
                                                          </div>
                                                        </div>
                                                      </div>
                                                      <!-- ################################################################################################-->
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </main>
  </div>